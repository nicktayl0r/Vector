<%= link_to "Create New User", new_user_path %>

<ul>
<% @users.each do |user| %>
    <li>
        <% user_id = (user.account_type.to_s + " - " + user.email.to_s) %>
        <%= link_to user_id, user_path(user) %> - <%= link_to "delete", user_path(user), method: :delete %> | <%= link_to "edit", edit_user_path(user) %>
    </li>
<% end %>
</ul>
<%= link_to "return to admin dashboard", admin_path %>